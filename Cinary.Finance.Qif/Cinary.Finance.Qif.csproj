<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Ahmet Cavus</Authors>
    <Company>Cinary</Company>
    <Description>A Qif Reader for the .Net Standard library</Description>
    <PackageLicenseUrl></PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/AhmetCavus/Qif.Reader</PackageProjectUrl>
    <RepositoryUrl>https://github.com/AhmetCavus/Qif.Reader.git</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
    <PackageTags>Finance Qif Reader Parser</PackageTags>
    <Version>1.3.1.0</Version>
